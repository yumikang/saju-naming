{
  "build": {
    "env": {
      "ENABLE_VC_BUILD": "1"
    }
  },
  "installCommand": "npm install",
  "buildCommand": "npm run build", 
  "devCommand": "npm run dev",
  "outputDirectory": "build/client",
  "functions": {
    "build/server/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/build/server/index.js"
    }
  ]
}